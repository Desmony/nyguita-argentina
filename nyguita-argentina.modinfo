<?xml version="1.0" encoding="UTF-8"?>
<Mod id="nyguita-argentina" version="1" xmlns="ModInfo">
    <Properties>
        <Name>Nyguita's Argentina</Name>
        <Description>Adds [B]Argentina[/B] as an Modern Age civilization</Description>
        <Authors>Nyguita</Authors>
		<SpecialThanks>izica, orion, Civ'ed, slothoth</SpecialThanks>
        <Package>Mod</Package>
        <Version>1.1.0</Version>
        <AffectsSavedGames>1</AffectsSavedGames>
    </Properties>
    <References>
		<Mod id="bolivar" title="LOC_MODULE_BOLIVAR_NAME"/>
		<Mod id="seelingcat-rosetta" title=""/>
		<Mod id="ged-ynamp" title=""/>
	</References>
    <ActionCriteria>
		<Criteria id="antiquity-age-current">
			<AgeInUse>AGE_ANTIQUITY</AgeInUse>
		</Criteria>
		<Criteria id="exploration-age-current">
			<AgeInUse>AGE_EXPLORATION</AgeInUse>
		</Criteria>
		<Criteria id="exploration-age-persist" any="true">
			<AgeInUse>AGE_EXPLORATION</AgeInUse>
			<AgeInUse>AGE_MODERN</AgeInUse>
		</Criteria>
        <Criteria id="age-modern-current">
            <AgeInUse>AGE_MODERN</AgeInUse>
        </Criteria>
        <Criteria id="has_bolivar_dlc">
            <AgeInUse>bolivar</AgeInUse>
        </Criteria>
        <Criteria id="always">
            <AlwaysMet/>
        </Criteria>
		<Criteria all="true" id="seelingcat-rosetta-enable">
            <ModInUse>seelingcat-rosetta</ModInUse>
        </Criteria>
		<Criteria all="true" id="ged-ynamp-enable">
            <ModInUse>ged-ynamp</ModInUse>
        </Criteria>
    </ActionCriteria>
    <ActionGroups>
        <ActionGroup id="rosetta_compatibility" scope="game" criteria="seelingcat-rosetta-enable">
            <Properties>
				<LoadOrder>100</LoadOrder>
			</Properties>
			<Actions>	
                <UpdateDatabase>
					<Item>rosetta/rosetta.sql</Item>
                </UpdateDatabase>
            </Actions>
		</ActionGroup>
		<ActionGroup id="ynamp_compatibility" scope="game" criteria="ged-ynamp-enable">
            <Properties>
				<LoadOrder>100</LoadOrder>
			</Properties>
			<Actions>	
                <UpdateDatabase>
					<Item>ynamp/ynamp.xml</Item>
                </UpdateDatabase>
            </Actions>
		</ActionGroup>
        <ActionGroup id="game-always" scope="game" criteria="always">
            <Properties>
                <LoadOrder>500</LoadOrder>
            </Properties>
            <Actions>
                <UpdateText>
                    <Item>civilizations/nyguita-argentina/localizations.sql</Item>
                    <Item>units/nyguita-gaucho/localizations.sql</Item>
                    <Item>units/nyguita-porteno/localizations.sql</Item>
                    <Item>units/nyguita-gaucho-2/localizations.sql</Item>
                    <Item>units/nyguita-gaucho-3/localizations.sql</Item>
                    <Item>civilization-unlocks/from-inca/localizations.sql</Item>
                    <Item>civilization-unlocks/from-spain/localizations.sql</Item>
                    <Item>civilization-unlocks/has-3-llamas/localizations.sql</Item>
                    <Item>civilization-unlocks/base_leaders_unlock/localizations.sql</Item>
                    <Item>traditions/nyguita-caudillism/localizations.sql</Item>
