                    <Item>traditions/nyguita-nuevo-tango/localizations.sql</Item>
                    <Item>progression-trees/nyguita-argentina/localizations.sql</Item>
                    <Item>unique-quarters/nyguita-plaza-de-mayo/localizations.sql</Item>
                </UpdateText>
                <UpdateDatabase>
                    <Item>civilizations/nyguita-argentina/legacy.sql</Item>
                    <Item>civilization-unlocks/from-inca/game.sql</Item>
                    <Item>civilization-unlocks/from-spain/game.sql</Item>
                    <Item>civilization-unlocks/has-3-llamas/game.sql</Item>
                    <Item>civilization-unlocks/base_leaders_unlock/game.sql</Item>
